{
  "version": 2,
  "builds": [
    {
      "src": "ignored",
      "use": "@vercel/docker",
      "config": { "maxLambdaSize": "500mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "30 13 * * *"
    }
  ]
}
