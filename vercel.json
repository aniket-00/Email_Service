{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "outputFileTracingIncludes": ["api/**/*.py"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "30 13 * * *"
    }
  ]
}